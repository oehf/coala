<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="eHF Checkstyle Configuration" location="http://devbuild008.de.icw.int/checkstyle/checkstyle_config_ehf.xml" type="remote" description="">
        <additional-data name="cache-file-location" value="eHF Checkstyle Configuration_local_cache.xml"/>
        <additional-data name="cache-file" value="true"/>
    </local-check-config>
    <fileset name="non-generated-sources" enabled="true" check-config-name="eHF Checkstyle Configuration" local="true">
        <file-match-pattern match-pattern="src.main.java.*" include-pattern="true"/>
        <file-match-pattern match-pattern="src.test.java.*" include-pattern="true"/>
    </fileset>
    <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
