<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	version="1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loginAccepted</from-outcome>
			<to-view-id>/patientSearch.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	

	<factory>
		<!-- <exception-handler-factory>exceptions.JSFExceptionHandlerFactory</exception-handler-factory> -->
	</factory>

</faces-config>
